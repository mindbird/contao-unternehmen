{
  "name": "mindbird/contao-unternehmen",
  "description": "Simple list of companies bundled in archives",
  "minimum-stability": "stable",
  "license": "LGPL-3.0-or-later",
  "keywords":["contao", "company", "unternehmen", "katalog", "sponsoren", "umkreissuche", "postleitzahl", "firmen"],
  "type":"contao-bundle",
  "authors": [
    {
      "name": "mindbird",
      "email": "info@mindbird.de"
    }
  ],
  "require": {
    "contao/core-bundle": "^5.6"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Mindbird\\Contao\\Company\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Mindbird\\Contao\\Company\\ContaoManager\\Plugin"
  },
  "support": {
    "issues": "https://github.com/mindbird/contao-unternehmen/issues",
    "source": "https://github.com/mindbird/contao-unternehmen"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "contao/manager-plugin": true,
      "php-http/discovery": true
    }
  }
}
