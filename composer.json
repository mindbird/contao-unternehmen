{
  "name": "mindbird/contao-unternehmen",
  "description": "Simple list of companies bundled in archives",
  "minimum-stability": "stable",
  "license": "proprietary",
  "keywords":["contao", "company", "unternehmen", "katalog", "sponsoren"],
  "type":"contao-module",
  "authors": [
    {
      "name": "mindbird",
      "email": "info@mindbird.de"
    }
  ],
  "require": {
    "contao/core-bundle": "~4.4",
    "contao-community-alliance/composer-plugin": "~3.0"
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/unternehmen"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Company\\": "src/"
    }
  },
  "replace": {
    "contao-legacy/unternehmen": "self.version"
  }
}
